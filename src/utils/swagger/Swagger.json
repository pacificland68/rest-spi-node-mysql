  "schemes": [
    "http"
  ],
  "host": "localhost:9999",
  "basePath": "/api",
  "paths": {
    "/taskinfo": {
      "get": {
        "summary": "根据id获取任务",
        "description": "根据id获取任务",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "description": "task id that needs to be deleted",
            "required": true,
            "type": "string"
          },
          {
            "name": "currentPage",
            "in": "query",
            "description": "currentPage that needs to be deleted",
            "required": true,
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "pageSize that needs to be deleted",
            "required": true,
            "type": "string"
          }
        ]
      }
    },
    "/getAllTask": {
      "get": {
        "summary": "获取任务列表",
        "description": "获取任务列表",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "currentPage",
            "in": "query",
            "description": "currentPage that needs to be deleted",
            "required": true,
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "pageSize that needs to be deleted",
            "required": true,
            "type": "string"
          }
        ]
      }
    }
  },
  "definitions": {
    "todosResponse": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "task": {
          "type": "string"
        },
        "assignee": {
          "type": "string"
        },
        "status": {
          "type": "string"
        }
      }
    },
    "InvalidResponse": {
      "type": "object",
      "properties": {
        "statusCode": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}